  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "dev": "sls offline start --stage dev",
    "dev:auth": "nest start authentication --watch",
    "dev:authentication:tunnel": "lt --port 3001 --subdomain authentication-todo",
    "dev:partner": "nest start partner --watch",
    "dev:partner:tunnel": "lt --port 3004 --subdomain partner",
    "dev:client": "nest start client --watch",
    "dev:client:tunnel": "lt --port 3003 --subdomain todo-g-client",
    "dev:tunnel": "lt --port 3000 --subdomain todo-gobacklog",
    "dev:collaborator": "nest start collaborator --watch",
    "dev:collaborator:tunnel": "lt --port 3002 --subdomain collaborator",
    "dev:geolocation": "nest start geolocation --watch",
    "dev:geolocation:tunnel": "lt --port 3005 --subdomain geolocation-todo",
    "dev:payments": "nest start payments --watch",
    "dev:payments:tunnel": "lt --port 3006 --subdomain payments",
    "dev:search": "nest start search --watch",
    "dev:search:tunnel": "lt --port 3007 --subdomain search",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js  --watch",
    "start:debug": "nest start --debug --webpackPath webpack-hmr.config.js --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/todogreen/test/jest-e2e.json"
  },
